{
    "name": "knowledge-search-vscode",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "knowledge-search",
    "workspaceFolder": "/knowledge-search",
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-vscode-remote.remote-containers",
          "ms-azuretools.vscode-docker"
        ]
      },
      "settings": {
        "python.pythonPath": "/opt/conda/envs/knowledge-search-env/bin/python"
      }
    }
}